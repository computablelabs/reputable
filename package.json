{
  "name": "reputable",
  "version": "0.1.0",
  "description": "Redux state container for the Computable Protocol",
  "main": "index.js",
  "scripts": {
    "test": "jasmine-ts ./spec/**/*.spec.ts",
    "build": "npm run build:clean && ./node_modules/typescript/bin/tsc",
    "build:clean": "rm -rf ./dist/constants && rm -rf ./dist/interfaces && rm -rf ./dist/redux",
    "build:dry": "./node_modules/typescript/bin/tsc -noEmit",
    "test:init": "jasmine-ts init"
  },
  "keywords": [
    "Computable",
    "Ethereum",
    "Javascript",
    "Typescript",
    "Redux"
  ],
  "author": "Computable.io",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.5.0",
    "@types/underscore": "^1.8.8",
    "ganache-cli": "^6.1.4",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "computable": "github:computablelabs/computable.js",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "web3": "^1.0.0-beta.34"
  }
}
