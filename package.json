{
  "name": "@computable/reputable",
  "version": "0.3.0",
  "description": "Redux state container for the Computable Protocol",
  "main": "index.js",
  "author": "Computable.io",
  "license": "MIT",
  "keywords": [
    "Computable",
    "Ethereum",
    "Javascript",
    "Typescript",
    "Redux"
  ],
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=./spec/support/jasmine.json jasmine-ts",
    "build": "npm run build:clean && tsc -p ./tsbuild.json && cp -r ./src/@types/** ./dist/@types",
    "build:clean": "rm -rf ./dist",
    "build:dry": "tsc -noEmit -p ./tsbuild.json",
    "test:init": "jasmine-ts init"
  },
  "dependencies": {
    "computable": "github:computablelabs/computable.js",
    "ipfs-api": "^24.0.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.5.0",
    "@types/underscore": "^1.8.8",
    "@types/uuid": "^3.4.4",
    "@types/web3": "^1.0.10",
    "ganache-cli": "^6.1.4",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "ts-sinon": "^1.0.12",
    "typescript": "^3.0.1"
  }
}
